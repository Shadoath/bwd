<div class="small-12 columns">
  <h1>Welcome <%=current_user.user_name%></h1>
  <table>
    <thead>
      <th></th>
      <th>Name</th>
      <th>Email</th>
      <th>Approved?</th>
      <th>Editor?</th>
      <th>Admin?</th>
    </thead>
    <tbody>

    <% @users.each do |user| %>
      <tr>
        <td><%= link_to raw('<i class="fa fa-edit small"></i>'), edit_admin_user_path(user)%>
        <td><%= user.user_name %></td>
        <td><%= user.email %></td>
        <td><%= user.approved? ? 'Yes' : 'No' %></td>
        <td><%= user.is_editor ? 'Yes' : 'No' %></td>
        <td><%= user.is_admin ? 'Yes' : 'No' %></td>
        <!--     TODO strikes/warnings,      -->
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
