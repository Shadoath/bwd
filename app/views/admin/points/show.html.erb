<div class="row">
  <div class="small-12 columns">
    <p id="notice"><%= notice %></p>

    <p>
    <strong>User:</strong>
    <%= @point.user.user_name %>
    </p>

    <p>
    <strong>Object:</strong>
    <%= @point.object_class %>(<%= @point.object_id %>)
    </p>

    <p>
    <strong>Change type:</strong>
    <%= @point.display_change_type %>
    </p>

    <p>
    <strong>Worth:</strong>
    <%= @point.worth %>
    </p>

    <%= link_to 'Back', admin_points_path %>
  </div>
</div>
