<%= simple_form_for(@create_user_favorite) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :is_liked %>
    <%= f.input :metta_id %>
    <%= f.input :type_id %>
    <%= f.input :comment %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
