<div class="row" id="rating-reviews-<%=review.id%>" >
  <div class="small-4 columns image-wrapper">
    <%= link_to PhotoDecorator.decorate(reviewed.photos.sample).display, edit_review_path(review) %>
  </div>
  <div class="small-8 columns">
    <h3 style="float:left;">
      <%= link_to reviewed.name, edit_review_path(review) %>
    </h3>
  </div>
  <div class="small-12 columns">
    <p align="none">
      <% if review.review_type == "Item" %>
        <%= link_to Restaurant.find(reviewed.restaurant_id).name, restaurant_path(reviewed.restaurant_id) %>
      <% else %>
        <%= review.review_type %>
      <% end %>
    </p>
  </div>
</div>
